<!DOCTYPE HTML>
<html lang="zh">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link href="../css/bootstrap.min.css" rel="stylesheet" />
    
    <link href="../css/main.css" rel="stylesheet" />
    <link href="../img/favicon.ico" type="image/x-icon" rel="shortcut icon">
    <title>诊间收费</title>
</head>

<body>
    <!-- Fixed navbar -->
    <nav class="navbar navbar-inverse navbar-fixed-top" id="topMainBox">
    </nav>

    <!-- Begin page content -->
    <div class="container content-middle">
        <div class="row">
            <div class="col-sm-3" id="leftMainBox">
            </div>

            <div class="col-sm-9 pay_mragin" id="loading_end">
                <div class="bmargin1">
                    <div>
                        <div class="page-title">收费条目</div>
                    </div>
                </div>

                <div class="list-group">
                    <div class="list-group-item">
                        <div class="filter">
                            <div class="filter-label pull-left">项目</div>
                            <div class="filter-item-wrap" id="pay-start-group">
                            </div>
                        </div>
                    </div>
                </div>
                <div class="panel panel-default">
                    <div class="panel-heading w-bg pay-head-border-bottom">
                        <h4 class="panel-title">收费项目</h4>
                    </div>
                    <div class="list-group" id="clinicServer-list">
                        <li class="list-group-item"><span>请选择收费项目</span><span class="pull-right"></span></li>
                    </div>
                </div>

            </div>
        </div>
    </div>

    <div class="gouwuche">
        <div class="container">
            <div class=row>
                <div class="col-sm-offset-3 col-sm-9 col-xs-12 ">
                    <div class="panel panel-default bmargin0">
                        <div class="panel-heading clearfix" style="display:none;"><!-- hide 会卡用display:none; -->
                            <div class="pull-left">购物车</div>
                            <div class="pull-right pointer" id="clearAllBtn">
                                        <i class="glyphicon glyphicon-trash"></i>清空
                                    </div>
                        </div>

                        <ul class="list-group service-list" style="display:none;" id="serviceList"> <!-- hide 会卡用display:none; -->
                        </ul>

                        <div class="panel-footer clearfix">
                            <div class="pay-start-img pointer" id="pay-car">
                                <div class="badge pay-start-badge " id="sum">0</div>
                            </div>
                            <div class="pull-right"><button type="button" class="btn btn-success tmargin" id="subBtn">提交</button></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- Modal -->
    <div class="modal fade dialog-box" id="PersonName" tabindex="-1" role="dialog" aria-labelledby="PersonName" >
      <div class="modal-dialog" role="document">
        <div class="modal-content">
          <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            <h4 class="modal-title" id="myModalLabel">选择收费对象</h4>
          </div>
          <div class="modal-body">
            <!-- 弹出框表格-->
                <div class="form-group">
                    <select class="form-control" id="selectPatientname">
                        <option>-请选择收费对象-</option>
                    </select>
                </div>
          </div>
          <div class="modal-footer">
            <button type="button" class="btn btn-default" data-dismiss="modal">取消</button>
            <button type="button" class="btn btn-primary resbg-table-color no-border" id="saveBtn">确定</button>
          </div>
        </div>
      </div>
    </div>
    
    <div class="container" id="loading">
        <div class="loading_box">
            <div class="spinner">
              <div class="spinner-container container1">
                <div class="circle1"></div>
                <div class="circle2"></div>
                <div class="circle3"></div>
                <div class="circle4"></div>
              </div>
              <div class="spinner-container container2">
                <div class="circle1"></div>
                <div class="circle2"></div>
                <div class="circle3"></div>
                <div class="circle4"></div>
              </div>
              <div class="spinner-container container3">
                <div class="circle1"></div>
                <div class="circle2"></div>
                <div class="circle3"></div>
                <div class="circle4"></div>
              </div>
            </div>
        </div>
    </div>
    <footer class="footer">

        <script src="../js/lib/jq-sh-bt.js " ></script>
        
        <script src="../js/pay_start.js" type="text/javascript"></script>
    </footer>

</body>

</html>