/* toast 样式*/

.toast-success {
    position: fixed;
    z-index: 10000;
    width: 7.6em;
    min-height: 7.6em;
    top: 50%;
    left: 50%;
    margin-left: -3.8em;
    margin-top: -3.8em;
    background: rgba(40, 40, 40, 0.75);
    text-align: center;
    border-radius: 5px;
    color: #FFFFFF;
}

.toast-success-icon {
    margin: 22px 0 0;
    display: block;
}

.toast-success-icon:before {
    content: '\EA08';
    color: #FFFFFF;
    font-size: 55px;
}

.toast_content {
    margin: 0 0 15px;
}

.loading_toast .toast_content {
    margin-top: 64%;
    font-size: 14px;
}

