/* mobile, hide and fixed */

@media screen and (max-width: 767px) {
    #sidebar {
        position: fixed;
        top: 51px;
        bottom: 0;
        left: 0;
        z-index: 1000;
        padding: 20px;
        overflow-x: hidden;
        overflow-y: auto;
        /* Scrollable contents if viewport is shorter than content. */
        background: #f5f5f5;
        border-right: 1px solid #ccc;
        display: none;
    }
    .mobile-hide {
        display: none;
    }
}

@media screen and (min-width: 768px) {
    .pc-hide {
        display: none;
    }
}

@media print {
    a[href]:after {
        content: none !important;
    }
}


